{
  "clusterLock": {
    "defaultClusterLockKeyspace": "my_cluster_lock_keyspace",
    "defaultClusterLockTable": "my_lock_table"
  },
  "keyspaces": {
    "my_keyspace_1": {
      "class": "NetworkTopologyStrategy",
      "datacenter1": "1"
    },
    "my_second_keyspace": {
      "class": "NetworkTopologyStrategy",
      "datacenter1": "2",
      "datacenter2": "3"
    },
    "_default": {
      "class": "SimpleStrategy",
      "replication_factor": "1"
    }
  }
}