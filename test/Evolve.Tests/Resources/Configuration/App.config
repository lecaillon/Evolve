<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <connectionStrings>
    <add name="cnn1" connectionString="Server=${EVOLVE_HOST};Port=5432;Database=myDataBase;User Id=${EVOLVE_DB_USER};Password=${EVOLVE_DB_PWD};" />
  </connectionStrings>
  <appSettings>
    <add key="Evolve.ConnectionString" value="cnn1" />
    <add key="Evolve.Database" value="postgresql" />
    <add key="Evolve.Locations" value="migration;dataset;" />
    <add key="Evolve.EraseDisabled" value="true" />
    <add key="Evolve.Command" value="Erase" />
    <add key="Evolve.EraseOnValidationError" value="True" />
    <add key="Evolve.Encoding" value="utf-16" />
    <add key="Evolve.SqlMigrationPrefix" value="Ver" />
    <add key="Evolve.SqlMigrationSeparator" value="@" />
    <add key="Evolve.SqlMigrationSuffix" value=".query" />
    <add key="Evolve.Schemas" value="my_shema" />
    <add key="Evolve.MetadataTableSchema" value="my_metadata_schema" />
    <add key="Evolve.MetadataTableName" value="metadata_store" />
    <add key="Evolve.PlaceholderPrefix" value="@{" />
    <add key="Evolve.PlaceholderSuffix" value="@}" />
    <add key="Evolve.TargetVersion" value="2_1_0" />
    <add key="Evolve.StartVersion" value="1_1_0" />
    <add key="Evolve.EnableClusterMode" value="true" />
    <add key="Evolve.OutOfOrder" value="true" />
    <add key="Evolve.CommandTimeout" value="200" />
    <add key="Evolve.EmbeddedResourceAssemblies" value="Evolve.dll;Evolve.Tests.dll" />
    <add key="Evolve.EmbeddedResourceFilters" value="Evolve.Resources;Evolve.Tests.Resources;" />

    <add key="Evolve.Placeholder.Schema" value="my_schema" />
    <add key="Evolve.Placeholder.Pwd" value="password" />
  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-11.0.0.0" newVersion="11.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Buffers" publicKeyToken="cc7b13ffcd2ddd51" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.0.3.0" newVersion="4.0.3.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.ValueTuple" publicKeyToken="cc7b13ffcd2ddd51" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.0.3.0" newVersion="4.0.3.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>